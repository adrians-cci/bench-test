<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.12">
  <POU Name="P_M01_RS02" Id="{945091b1-fbe3-48ae-be8f-438de18d2b40}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM P_M01_RS02
VAR_INPUT
	stop_rs_automode													: BOOL;
	clear_rs_pause												: BOOL;
	close_valves														: BOOL;
	close_all_doors														: BOOL;
	open_all_doors														: BOOL;
	retract_all_clamps													: BOOL;
	extend_all_clamps													: BOOL;
	open_repres_valves													: BOOL;
	close_repres_valves													: BOOL;
	co2_sensors_off														: BOOL;
	co2_inside_sensors_on												: BOOL;
	co2_outside_sensors_on												: BOOL;
	fan_vfd_percent														: REAL		:= 0;
END_VAR
VAR_OUTPUT
	repres_valves_clear													: BOOL;
	average_pressure_within_ambient										: BOOL;
	reactor_done														: BOOL;
	utility_error														: BOOL;
	cancel_reactor_set													: BOOL;
	cancel_all_reactor_sets												: BOOL;
	inlet_air_high_temp_count											: INT;
	open_inlet_doors													: BOOL;
	open_outlet_doors													: BOOL;
	close_inlet_doors													: BOOL;
	close_outlet_doors													: BOOL;
	open_doors															: BOOL;
	close_doors															: BOOL;
	inlet_door_clamps_extend											: BOOL;
	outlet_door_clamps_extend											: BOOL;
	inlet_door_clamps_retract											: BOOL;
	outlet_door_clamps_retract											: BOOL;
	extend_clamps														: BOOL;
	retract_clamps														: BOOL;
END_VAR
VAR
	R_S																	: Reactor_Set;

	fb_RS																: FB_Reactor_Set();
	fb_RS_IO															: FB_Reactor_Set_IO();
	fb_R1_IO															: FB_Reactor_IO();
	fb_R2_IO															: FB_Reactor_IO();
	fb_R3_IO															: FB_Reactor_IO();
	fb_R4_IO															: FB_Reactor_IO();
	
	fb_Fan_Vfd															: FB_Analog_Out();
	
	rAveragePressure													: REAL;
	rValve																: REAL;
	rPos																: REAL;
	rFanOldVfdPercent													: REAL		:= 0;
	
	iCount																: INT;
	iVacuumStatus														: INT;
	iError																: INT;
	
	bR1OverMaxPressure													: BOOL;
	bR2OverMaxPressure													: BOOL;
	bR3OverMaxPressure													: BOOL;
	bR4OverMaxPressure													: BOOL;
	
	bR1InletCO2Error													: BOOL;
	bR1InletH2OError													: BOOL;
	bR1OutletCO2Error													: BOOL;
	bR1OutletH2OError													: BOOL;
	bR1AirFlowError														: BOOL;
	bR1FilterDiffPresError												: BOOL;
	bR1SorbentDiffPresError												: BOOL;
	bR1FanDiffPresError													: BOOL;
	bR1ReactorPresError													: BOOL;
	
	bR2InletCO2Error													: BOOL;
	bR2InletH2OError													: BOOL;
	bR2OutletCO2Error													: BOOL;
	bR2OutletH2OError													: BOOL;
	bR2AirFlowError														: BOOL;
	bR2FilterDiffPresError												: BOOL;
	bR2SorbentDiffPresError												: BOOL;
	bR2FanDiffPresError													: BOOL;
	bR2ReactorPresError													: BOOL;
	
	bR3InletCO2Error													: BOOL;
	bR3InletH2OError													: BOOL;
	bR3OutletCO2Error													: BOOL;
	bR3OutletH2OError													: BOOL;
	bR3AirFlowError														: BOOL;
	bR3FilterDiffPresError												: BOOL;
	bR3SorbentDiffPresError												: BOOL;
	bR3FanDiffPresError													: BOOL;
	bR3ReactorPresError													: BOOL;
	
	bR4InletCO2Error													: BOOL;
	bR4InletH2OError													: BOOL;
	bR4OutletCO2Error													: BOOL;
	bR4OutletH2OError													: BOOL;
	bR4AirFlowError														: BOOL;
	bR4FilterDiffPresError												: BOOL;
	bR4SorbentDiffPresError												: BOOL;
	bR4FanDiffPresError													: BOOL;
	bR4ReactorPresError													: BOOL;
	
	stop_reactor_set													: BOOL;
	clear_reactor_set_pause												: BOOL;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[// Phase 0 - P_M01_RS02
// Rev 1.0 - May 11, 2024

giEvacuationErrorRS														:= SEL(giEvacuationErrorRS = 2, giEvacuationErrorRS, 0);
giDesorptionErrorRS														:= SEL(giDesorptionErrorRS = 2, giDesorptionErrorRS, 0);
giSafeShutdown1RS														:= SEL(giSafeShutdown1RS = 2, giSafeShutdown1RS, 0);
giSafeShutdown23RS														:= SEL(giSafeShutdown23RS = 2, giSafeShutdown23RS, 0);

stop_reactor_set														:= stop_rs_automode;
stop_rs_automode														:= FALSE;
clear_reactor_set_pause													:= clear_rs_pause;
clear_rs_pause															:= FALSE;

A_Reactor_Set();

// REACTOR OUTPUTS
IF open_repres_valves OR R_S.open_repres_valves THEN
	fb_R1_IO.bRepresValve												:= TRUE;
	fb_R2_IO.bRepresValve												:= TRUE;
	fb_R3_IO.bRepresValve												:= TRUE;
	fb_R4_IO.bRepresValve												:= TRUE;
	open_repres_valves													:= FALSE;
END_IF
IF close_repres_valves OR R_S.close_repres_valves THEN
	fb_R1_IO.bRepresValve												:= FALSE;
	fb_R2_IO.bRepresValve												:= FALSE;
	fb_R3_IO.bRepresValve												:= FALSE;
	fb_R4_IO.bRepresValve												:= FALSE;
	close_repres_valves													:= FALSE;
END_IF

repres_valves_clear														:= fb_R1_IO.repres_valve_clear AND fb_R2_IO.repres_valve_clear AND fb_R3_IO.repres_valve_clear AND fb_R4_IO.repres_valve_clear;
IF NOT repres_valves_clear THEN
	R_S.repres_valve_error												:= SEL(fb_R4_IO.repres_valve_clear, CONCAT(_R4, gsRepressurizationValveError), '');
	R_S.repres_valve_error												:= SEL(fb_R3_IO.repres_valve_clear, CONCAT(_R3, gsRepressurizationValveError), R_S.repres_valve_error);
	R_S.repres_valve_error												:= SEL(fb_R2_IO.repres_valve_clear, CONCAT(_R2, gsRepressurizationValveError), R_S.repres_valve_error);
	R_S.repres_valve_error												:= SEL(fb_R1_IO.repres_valve_clear, CONCAT(_R1, gsRepressurizationValveError), R_S.repres_valve_error);
ELSE
	R_S.repres_valve_error												:= '';
END_IF

fb_Fan_Vfd(Input1:= fan_vfd_percent, Input2:= R_S.fan_vfd_percent);
IF fan_vfd_percent <> rFanOldVfdPercent THEN
	fb_R1_IO.fan_vfd_input											:= fan_vfd_percent;
	fb_R2_IO.fan_vfd_input											:= fan_vfd_percent;
	fb_R3_IO.fan_vfd_input											:= fan_vfd_percent;
	fb_R4_IO.fan_vfd_input											:= fan_vfd_percent;
	rFanOldVfdPercent													:= fan_vfd_percent;
END_IF
		
A_Reactor1_IO();
A_Reactor2_IO();
A_Reactor3_IO();
A_Reactor4_IO();

inlet_air_high_temp_count												:= fb_R1_IO.adsorption_high_temp_count + fb_R2_IO.adsorption_high_temp_count + fb_R3_IO.adsorption_high_temp_count + fb_R4_IO.adsorption_high_temp_count;
R_S.inlet_air_high_temp_count											:= inlet_air_high_temp_count;
R_S.desorption_high_temp_count											:= fb_R1_IO.desorption_high_temp_count + fb_R2_IO.desorption_high_temp_count + fb_R3_IO.desorption_high_temp_count + fb_R4_IO.desorption_high_temp_count;
R_S.vacuum_cooling_high_temp_count										:= fb_R1_IO.vacuum_cooling_high_temp_count + fb_R2_IO.vacuum_cooling_high_temp_count + fb_R3_IO.vacuum_cooling_high_temp_count + fb_R4_IO.vacuum_cooling_high_temp_count;
R_S.sorbent_high_temp													:= F_HighestReal(fb_R1_IO.sorbent_high_temp, fb_R2_IO.sorbent_high_temp, fb_R3_IO.sorbent_high_temp, fb_R4_IO.sorbent_high_temp);

m01_rs2_r1.reactor_wall_temp											:= F_Temperature(m01_rs2_r1.TT_05, m01_rs2_r1.reactor_wall_temp_offset);
m01_rs2_r1.outlet_line_temp												:= F_Temperature(m01_rs2_r1.TT_06, m01_rs2_r1.outlet_line_temp_offset);
m01_rs2_r2.reactor_wall_temp											:= F_Temperature(m01_rs2_r2.TT_05, m01_rs2_r2.reactor_wall_temp_offset);
m01_rs2_r2.outlet_line_temp												:= F_Temperature(m01_rs2_r2.TT_06, m01_rs2_r2.outlet_line_temp_offset);
m01_rs2_r3.reactor_wall_temp											:= F_Temperature(m01_rs2_r3.TT_05, m01_rs2_r3.reactor_wall_temp_offset);
m01_rs2_r3.outlet_line_temp												:= F_Temperature(m01_rs2_r3.TT_06, m01_rs2_r3.outlet_line_temp_offset);
m01_rs2_r4.reactor_wall_temp											:= F_Temperature(m01_rs2_r4.TT_05, m01_rs2_r4.reactor_wall_temp_offset);
m01_rs2_r4.outlet_line_temp												:= F_Temperature(m01_rs2_r4.TT_06, m01_rs2_r4.outlet_line_temp_offset);

// CO2 SENSOR HANDLING
IF co2_sensors_off OR R_S.co2_sensors_off THEN
	fb_R1_IO.co2_sensor_config											:= 0;
	fb_R2_IO.co2_sensor_config											:= 0;
	fb_R3_IO.co2_sensor_config											:= 0;
	fb_R4_IO.co2_sensor_config											:= 0;
	co2_sensors_off														:= FALSE;
ELSIF co2_inside_sensors_on OR R_S.co2_inside_sensors_on THEN
	fb_R1_IO.co2_sensor_config											:= 1;
	fb_R2_IO.co2_sensor_config											:= 1;
	fb_R3_IO.co2_sensor_config											:= 1;
	fb_R4_IO.co2_sensor_config											:= 1;
	co2_inside_sensors_on												:= FALSE;
ELSIF co2_outside_sensors_on OR R_S.co2_outside_sensors_on THEN
	fb_R1_IO.co2_sensor_config											:= 2;
	fb_R2_IO.co2_sensor_config											:= 2;
	fb_R3_IO.co2_sensor_config											:= 2;
	fb_R4_IO.co2_sensor_config											:= 2;
	co2_outside_sensors_on												:= FALSE;
END_IF
 
// DOORS & CLAMPS
inlet_door_clamps_extend												:= FALSE;	
outlet_door_clamps_extend												:= FALSE;
inlet_door_clamps_retract												:= FALSE;	
outlet_door_clamps_retract												:= FALSE;
extend_clamps															:= FALSE;
retract_clamps															:= FALSE;
IF extend_all_clamps OR R_S.extend_all_clamps THEN
	extend_clamps														:= TRUE;
	extend_all_clamps													:= FALSE;
END_IF
IF retract_all_clamps OR R_S.retract_all_clamps THEN
	retract_clamps														:= TRUE;	
	retract_all_clamps													:= FALSE;
END_IF
R_S.all_clamps_extended													:= fb_R1_IO.door_clamps_extended AND fb_R2_IO.door_clamps_extended AND fb_R3_IO.door_clamps_extended AND fb_R4_IO.door_clamps_extended; 
R_S.all_clamps_retracted												:= fb_R1_IO.door_clamps_retracted AND fb_R2_IO.door_clamps_retracted AND fb_R3_IO.door_clamps_retracted AND fb_R4_IO.door_clamps_retracted;

open_inlet_doors														:= FALSE;
open_outlet_doors														:= FALSE;
close_inlet_doors														:= FALSE;
close_outlet_doors														:= FALSE;
open_doors																:= FALSE;
close_doors																:= FALSE;
IF open_all_doors OR R_S.open_all_doors THEN
	open_doors															:= TRUE;
	open_all_doors														:= FALSE;
END_IF
fb_R1_IO.close_doors													:= FALSE;
IF close_all_doors OR R_S.close_all_doors THEN
	close_doors															:= TRUE;
	close_all_doors														:= FALSE;
END_IF
R_S.all_doors_open														:= fb_R1_IO.doors_open AND fb_R2_IO.doors_open AND fb_R3_IO.doors_open AND fb_R4_IO.doors_open;
R_S.all_doors_closed													:= fb_R1_IO.doors_closed AND fb_R2_IO.doors_closed AND fb_R3_IO.doors_closed AND fb_R4_IO.doors_closed;

m01_rs2_r1.inlet_co2													:= F_AnalogIn(m01_rs2_r1.AT_01, m01_rs2_r1.inlet_co2_low, m01_rs2_r1.inlet_co2_high, m01_rs2_r1.inlet_co2_min, m01_rs2_r1.inlet_co2_offset);
bR1InletCO2Error														:= P_Alarms.M_Check_Error(m01_rs2_r1.inlet_co2_error, bR1InletCO2Error, CONCAT(_RS01, _R1), gsInletCO2SensorError, gsProcessWillContinue);
m01_rs2_r1.inlet_h2o													:= F_AnalogIn(m01_rs2_r1.AT_02, m01_rs2_r1.inlet_h2o_low, m01_rs2_r1.inlet_h2o_high, m01_rs2_r1.inlet_h2o_min, m01_rs2_r1.inlet_h2o_offset);
bR1InletH2OError														:= P_Alarms.M_Check_Error(m01_rs2_r1.inlet_h2o_error, bR1InletH2OError, CONCAT(_RS01, _R1), gsInletH2OSensorError, gsProcessWillContinue);
m01_rs2_r1.outlet_co2													:= F_AnalogIn(m01_rs2_r1.AT_03, m01_rs2_r1.outlet_co2_low, m01_rs2_r1.outlet_co2_high, m01_rs2_r1.outlet_co2_min, m01_rs2_r1.outlet_co2_offset);
bR1OutletCO2Error														:= P_Alarms.M_Check_Error(m01_rs2_r1.outlet_co2_error, bR1OutletCO2Error, CONCAT(_RS01, _R1), gsOutletCO2SensorError, gsProcessWillContinue);
m01_rs2_r1.outlet_h2o													:= F_AnalogIn(m01_rs2_r1.AT_04, m01_rs2_r1.outlet_h2o_low, m01_rs2_r1.outlet_h2o_high, m01_rs2_r1.outlet_h2o_min, m01_rs2_r1.outlet_h2o_offset);
bR1OutletH2OError														:= P_Alarms.M_Check_Error(m01_rs2_r1.outlet_h2o_error, bR1OutletH2OError, CONCAT(_RS01, _R1), gsOutletH2OSensorError, gsProcessWillContinue);
m01_rs2_r1.air_flow														:= F_AnalogIn(m01_rs2_r1.FT_01, m01_rs2_r1.air_flow_low, m01_rs2_r1.air_flow_high, m01_rs2_r1.air_flow_min, m01_rs2_r1.air_flow_offset);
bR1AirFlowError															:= P_Alarms.M_Check_Error(m01_rs2_r1.air_flow_error, bR1AirFlowError, CONCAT(_RS01, _R1), gsAirFlowSensorError, gsProcessWillContinue);
m01_rs2_r1.filter_diff_pres												:= F_AnalogIn(m01_rs2_r1.PDT_01, m01_rs2_r1.filter_diff_pres_low, m01_rs2_r1.filter_diff_pres_high, m01_rs2_r1.filter_diff_pres_min, m01_rs2_r1.filter_diff_pres_offset);
bR1FilterDiffPresError													:= P_Alarms.M_Check_Error(m01_rs2_r1.filter_diff_pres_error, bR1FilterDiffPresError, CONCAT(_RS01, _R1), gsFilterDiffPresSensorError, gsProcessWillContinue);
m01_rs2_r1.sorbent_diff_pres											:= F_AnalogIn(m01_rs2_r1.PDT_02, m01_rs2_r1.sorbent_diff_pres_low, m01_rs2_r1.sorbent_diff_pres_high, m01_rs2_r1.sorbent_diff_pres_min, m01_rs2_r1.sorbent_diff_pres_offset,);
bR1SorbentDiffPresError													:= P_Alarms.M_Check_Error(m01_rs2_r1.sorbent_diff_pres_error, bR1SorbentDiffPresError, CONCAT(_RS01, _R1), gsSorbentDiffPresSensorError, gsProcessWillContinue);
m01_rs2_r1.fan_diff_pres												:= F_AnalogIn(m01_rs2_r1.PDT_03, m01_rs2_r1.fan_diff_pres_low, m01_rs2_r1.fan_diff_pres_high, m01_rs2_r1.fan_diff_pres_min, m01_rs2_r1.fan_diff_pres_offset);
bR1FanDiffPresError														:= P_Alarms.M_Check_Error(m01_rs2_r1.fan_diff_pres_error, bR1FanDiffPresError, CONCAT(_RS01, _R1), gsFanDiffPresSensorError, gsProcessWillContinue);
m01_rs2_r1.reactor_pres													:= F_AnalogIn(m01_rs2_r1.PT_01, m01_rs2_r1.reactor_pres_low, m01_rs2_r1.reactor_pres_high, m01_rs2_r1.reactor_pres_min, m01_rs2_r1.reactor_pres_offset);
bR1ReactorPresError														:= P_Alarms.M_Check_Error(m01_rs2_r1.reactor_pres_error, bR1ReactorPresError, CONCAT(_RS01, _R1), gsReactorPresSensorError, gsProcessWillContinue);
R_S.R1_reactor_pres														:= m01_rs2_r1.reactor_pres;
R_S.R1_reactor_pres_error												:= bR1ReactorPresError;
bR1OverMaxPressure														:= NOT m01_rs2_r1.reactor_pres_error AND m01_rs2_r1.reactor_pres > m01.SP_maximum_safe_reactor_steam_pressure;

m01_rs2_r2.inlet_co2													:= F_AnalogIn(m01_rs2_r2.AT_01, m01_rs2_r2.inlet_co2_low, m01_rs2_r2.inlet_co2_high, m01_rs2_r2.inlet_co2_min, m01_rs2_r2.inlet_co2_offset);
bR2InletCO2Error														:= P_Alarms.M_Check_Error(m01_rs2_r2.inlet_co2_error, bR2InletCO2Error, CONCAT(_RS01, _R2), gsInletCO2SensorError, gsProcessWillContinue);
m01_rs2_r2.inlet_h2o													:= F_AnalogIn(m01_rs2_r2.AT_02, m01_rs2_r2.inlet_h2o_low, m01_rs2_r2.inlet_h2o_high, m01_rs2_r2.inlet_h2o_min, m01_rs2_r2.inlet_h2o_offset);
bR2InletH2OError														:= P_Alarms.M_Check_Error(m01_rs2_r2.inlet_h2o_error, bR2InletH2OError, CONCAT(_RS01, _R2), gsInletH2OSensorError, gsProcessWillContinue);
m01_rs2_r2.outlet_co2													:= F_AnalogIn(m01_rs2_r2.AT_03, m01_rs2_r2.outlet_co2_low, m01_rs2_r2.outlet_co2_high, m01_rs2_r2.outlet_co2_min, m01_rs2_r2.outlet_co2_offset);
bR2OutletCO2Error														:= P_Alarms.M_Check_Error(m01_rs2_r2.outlet_co2_error, bR2OutletCO2Error, CONCAT(_RS01, _R2), gsOutletCO2SensorError, gsProcessWillContinue);
m01_rs2_r2.outlet_h2o													:= F_AnalogIn(m01_rs2_r2.AT_04, m01_rs2_r2.outlet_h2o_low, m01_rs2_r2.outlet_h2o_high, m01_rs2_r2.outlet_h2o_min, m01_rs2_r2.outlet_h2o_offset);
bR2OutletH2OError														:= P_Alarms.M_Check_Error(m01_rs2_r2.outlet_h2o_error, bR2OutletH2OError, CONCAT(_RS01, _R2), gsOutletH2OSensorError, gsProcessWillContinue);
m01_rs2_r2.air_flow														:= F_AnalogIn(m01_rs2_r2.FT_01, m01_rs2_r2.air_flow_low, m01_rs2_r2.air_flow_high, m01_rs2_r2.air_flow_min, m01_rs2_r2.air_flow_offset);
bR2AirFlowError															:= P_Alarms.M_Check_Error(m01_rs2_r2.air_flow_error, bR2AirFlowError, CONCAT(_RS01, _R2), gsAirFlowSensorError, gsProcessWillContinue);
m01_rs2_r2.filter_diff_pres												:= F_AnalogIn(m01_rs2_r2.PDT_01, m01_rs2_r2.filter_diff_pres_low, m01_rs2_r2.filter_diff_pres_high, m01_rs2_r2.filter_diff_pres_min, m01_rs2_r2.filter_diff_pres_offset);
bR2FilterDiffPresError													:= P_Alarms.M_Check_Error(m01_rs2_r2.filter_diff_pres_error, bR2FilterDiffPresError, CONCAT(_RS01, _R2), gsFilterDiffPresSensorError, gsProcessWillContinue);
m01_rs2_r2.sorbent_diff_pres											:= F_AnalogIn(m01_rs2_r2.PDT_02, m01_rs2_r2.sorbent_diff_pres_low, m01_rs2_r2.sorbent_diff_pres_high, m01_rs2_r2.sorbent_diff_pres_min, m01_rs2_r2.sorbent_diff_pres_offset,);
bR2SorbentDiffPresError													:= P_Alarms.M_Check_Error(m01_rs2_r2.sorbent_diff_pres_error, bR2SorbentDiffPresError, CONCAT(_RS01, _R2), gsSorbentDiffPresSensorError, gsProcessWillContinue);
m01_rs2_r2.fan_diff_pres												:= F_AnalogIn(m01_rs2_r2.PDT_03, m01_rs2_r2.fan_diff_pres_low, m01_rs2_r2.fan_diff_pres_high, m01_rs2_r2.fan_diff_pres_min, m01_rs2_r2.fan_diff_pres_offset);
bR2FanDiffPresError														:= P_Alarms.M_Check_Error(m01_rs2_r2.fan_diff_pres_error, bR2FanDiffPresError, CONCAT(_RS01, _R2), gsFanDiffPresSensorError, gsProcessWillContinue);
m01_rs2_r2.reactor_pres													:= F_AnalogIn(m01_rs2_r2.PT_01, m01_rs2_r2.reactor_pres_low, m01_rs2_r2.reactor_pres_high, m01_rs2_r2.reactor_pres_min, m01_rs2_r2.reactor_pres_offset);
bR2ReactorPresError														:= P_Alarms.M_Check_Error(m01_rs2_r2.reactor_pres_error, bR2ReactorPresError, CONCAT(_RS01, _R2), gsReactorPresSensorError, gsProcessWillContinue);
R_S.R2_reactor_pres														:= m01_rs2_r2.reactor_pres;
R_S.R2_reactor_pres_error												:= bR2ReactorPresError;
bR2OverMaxPressure														:= NOT m01_rs2_r2.reactor_pres_error AND m01_rs2_r2.reactor_pres > m01.SP_maximum_safe_reactor_steam_pressure;

