iCount																	:= 0;
iVacuumStatus															:= 0;
iError																	:= 4;
IF NOT m01_rs2_r1.reactor_pres_error THEN
	rAveragePressure													:= rAveragePressure + m01_rs2_r1.reactor_pres;
	iVacuumStatus														:= F_LowMidHigh(m01_rs2_r1.reactor_pres, m01.SP_target_desorption_starting_pressure, m01.SP_maximum_desorption_starting_pressure, iVacuumStatus);
	iCount																:= iCount + 1;
	iError																:= iError - 1;
END_IF
IF NOT m01_rs2_r2.reactor_pres_error THEN
	rAveragePressure													:= rAveragePressure + m01_rs2_r2.reactor_pres;
	iVacuumStatus														:= F_LowMidHigh(m01_rs2_r2.reactor_pres, m01.SP_target_desorption_starting_pressure, m01.SP_maximum_desorption_starting_pressure, iVacuumStatus);
	iCount																:= iCount + 1;
	iError																:= iError - 1;
END_IF
IF NOT m01_rs2_r3.reactor_pres_error THEN
	rAveragePressure													:= rAveragePressure + m01_rs2_r3.reactor_pres;
	iVacuumStatus														:= F_LowMidHigh(m01_rs2_r3.reactor_pres, m01.SP_target_desorption_starting_pressure, m01.SP_maximum_desorption_starting_pressure, iVacuumStatus);
