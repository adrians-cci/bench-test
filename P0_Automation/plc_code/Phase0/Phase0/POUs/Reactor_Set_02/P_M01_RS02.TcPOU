	iVacuumStatus														:= F_LowMidHigh(m01_rs2_r1.reactor_pres, m01.SP_target_desorption_starting_pressure, m01.SP_maximum_desorption_starting_pressure, iVacuumStatus);
	iCount																:= iCount + 1;
	iError																:= iError - 1;
END_IF

R_S.average_reactor_pressure											:= SEL(iCount > 0, 0, rAveragePressure / iCount);
R_S.reactor_pressure_error												:= SEL(iError >= _MAX_REACTOR_ERRORS, '', F_Concat3(INT_TO_STRING(iError), ' ', gsReactorPresError));
R_S.reactor_vacuum_status												:= iVacuumStatus;
average_pressure_within_ambient											:= rAveragePressure >= P_M01.rMinPressureSP AND rAveragePressure <= P_M01.rMaxPressureSP;
R_S.average_reactor_pressure_within_ambient								:= average_pressure_within_ambient;
fb_RS_IO.reactor_over_max_pressure										:= bR1OverMaxPressure OR bR2OverMaxPressure OR bR3OverMaxPressure OR bR4OverMaxPressure;

R_S.R1_inlet_co2														:= m01_rs2_r1.inlet_co2;

R_S.R1_outlet_co2														:= m01_rs2_r1.outlet_co2;
]]></ST>
